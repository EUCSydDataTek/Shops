@page
@model MyApp.Namespace.ListModel

<table class="table">
    <thead>
        <tr>
            <th scope="col">Name</th>
            <th scope="col">Location</th>
            <th scope="col">Type</th>
        </tr>
    </thead>
    @foreach (var shop in Model.Shops)
    {
        <tr>
            <td>@shop.Name</td>
            <td>@shop.Location</td>
            <td>@shop.Type.Name</td>
        </tr>
    }
</table>